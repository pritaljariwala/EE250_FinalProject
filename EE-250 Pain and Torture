#EE-250 Pain and Torture Project

import time 
import grovepi

play_sensor = 14
volume_sensor = 15
playback = "paused"
grovepi.pinMode(play_sensor,"INPUT")
while True:
    try:
        current_reading = grovepi.analogRead(play_sensor)
        if playback = "paused" && current_reading > 700:
            playback = "play"
        if playback = "play" && current_reading > 700:
            playback = "paused"


